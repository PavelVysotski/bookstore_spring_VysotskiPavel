<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="bookCommand" class="com.belhard.controller.command.impl.BookCommand">
        <constructor-arg ref="bookService"/>
    </bean>
    <bean id="booksCommand" class="com.belhard.controller.command.impl.BooksCommand">
        <constructor-arg ref="bookService"/>
    </bean>
    <bean id="createBookCommand" class="com.belhard.controller.command.impl.CreateBookCommand">
        <constructor-arg ref="bookService"/>
    </bean>
    <bean id="userCommand" class="com.belhard.controller.command.impl.UserCommand">
        <constructor-arg ref="userService"/>
    </bean>
    <bean id="usersCommand" class="com.belhard.controller.command.impl.UsersCommand">
        <constructor-arg ref="userService"/>
    </bean>
    <bean id="bookService" class="com.belhard.service.impl.BookServiceImpl">
        <constructor-arg ref="bookDao"/>
    </bean>
    <bean id="userService" class="com.belhard.service.impl.UserServiceImpl">
        <constructor-arg ref="userDao"/>
    </bean>
    <bean id="bookDao" class="com.belhard.dao.book.BookDaoImpl"/>
    <bean id="userDao" class="com.belhard.dao.user.UserDaoImpl"/>

</beans>